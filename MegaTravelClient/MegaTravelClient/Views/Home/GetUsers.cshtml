
@using MegaTravelClient.Models

@model GetUsersResponseModel

<h2>List of Users</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>

        </tr>
    </thead>
    <tbody>
@foreach (var user in Model.userList)
{
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.Phone</td>
        </tr>
}  
</tbody>
</table>






